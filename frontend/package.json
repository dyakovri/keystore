{
  "name": "keystore-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "build:development": "vue-tsc && vite build --mode development",
    "build:testing": "vue-tsc && vite build --mode testing",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .ts,.vue .",
    "lint:fix": "eslint --ext .ts,.vue . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "stylelint": "stylelint \"**/*.{vue,css}\"",
    "stylelint:fix": "stylelint \"**/*.{vue,css}\" --fix"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "material-design-icons-iconfont": "^6.7.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.14"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^5.1.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.5"
  }
}
